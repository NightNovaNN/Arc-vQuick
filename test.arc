print "Starting Arc Test!"

var x = 5
print "Initial x:" x

if x > 3:
    print "x is greater than 3"
else:
    print "x is less than or equal to 3"

var y = 1
while y <= x:
    print "Counting:" y
    var y = y + 1

print "Loop finished!"

var z = x * 2 + y - 3
print "Final value of z:" z

print "Test complete!"
